echo "Enter first number:"
read num1
echo "Enter second number:"
read num2

sum=$((num1 +num2))
diff=$((num1 - num2))
product=$((num1 * num2))
if [ $num2 -ne 0 ];then
   quotient=$((num1 / num2))
else
   quotient="Undefined"
fi
echo "Sum: $sum"
echo "Difference: $diff"
echo "Product:$product"
echo "Quotient: $quotient"


